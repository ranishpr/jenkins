pipeline {
    agent none
    stages {
        stage('version') {
            steps {
                echo "version created"
            }
        }    
        stage('Tag') {
            steps {
                echo "Tagged"
            }
        }
        stage('Build') {
            steps {
                echo "Build successful"
            }
        }
        stage('Test') {
            steps {
                echo "Tested"
            }
        }
        stage('Upload') {
            steps {
                echo "uploaded"
            }
        }
    }
}
