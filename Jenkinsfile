pipeline {
    agent none
    stages {
        stage('Version') {
            steps {
                echo "Version created"
            }
        }    
        stage('Tag') {
            steps {
                echo "Tagged"
            }
        }
        stage('Build') {
            steps {
                echo "Build successful"
            }
        }
        stage('Test') {
            steps {
                echo "Tested"
            }
        }
        stage('Upload') {
            steps {
                echo "uploaded"
            }
        }
    }
}
